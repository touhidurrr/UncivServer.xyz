{
  // arktype
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "typescript.preferences.autoImportSpecifierExcludeRegexes": ["^(node:)?os$"],

  // cSpell
  "cSpell.words": ["arktype", "buymeacoffee", "eleventy"],

  // misc
  "[nunjucks]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },

  "[jinja-html]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  }
}
