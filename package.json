{
  "name": "uncivserver.xyz",
  "version": "4.40.0",
  "description": "An Open Source, Free to Play, Unciv Multiplayer Server written in Javascript",
  "author": "Md. Touhidur Rahman",
  "license": "BSD-3-Clause",
  "repository": "touhidurrr/UncivServer.xyz",
  "homepage": "https://github.com/touhidurrr/UncivServer.xyz#readme",
  "bugs": {
    "url": "https://github.com/touhidurrr/UncivServer.xyz/issues"
  },
  "keywords": [
    "Unciv",
    "Multiplayer",
    "Server",
    "Discord",
    "Turn",
    "Notifications",
    "Bun",
    "Elysia"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "lint": "eslint .",
    "format": "prettier --write '**/?*.*'",
    "wc": "find src tests -type f -exec wc {} +",
    "build": "bun scripts/build.bun.sh"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "@mongodb-js/zstd": "^2.0.1",
    "arktype": "^2.1.22",
    "bytes": "^3.1.2",
    "cache-control-parser": "^2.0.6",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.25",
    "elysia": "^1.4.5",
    "lru-cache": "^11.2.1",
    "mongoose": "^8.18.1",
    "node-cache": "^5.1.2",
    "openskill": "^4.1.0",
    "randomcryp": "^2.1.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@eslint/js": "^9.35.0",
    "@types/bun": "^1.2.22",
    "@types/bytes": "^3.1.5",
    "axios": "^1.12.2",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "trustedDependencies": [
    "@mongodb-js/zstd"
  ]
}
